run;


data cars1; ***getting max count of words in the MODEL variable for each record***;
	set cars;
	model = strip(compbl(model));
	spaces = 0; *count of spaces within model var value; 

	do i=1 to length(model); *counting of spaces within value;
		if strip(substr(model, i, 1)) = " " and i ne length(model) then spaces = spaces + 1;
	end; 

	count = spaces + 1; *we may get count of words. Count of spaces always less for one than count of words in the value;
run;

proc sql; ***getting macro var with max count of words in var per dataset***;
	select max(count) into : maxcnt
	from cars1;
run;

data cars2(drop=buffer i); ***separate model of cars to array***;
	length buffer $200;
	set cars1;
	array model_sp {*} $200 val1-val%eval(&maxcnt);
	buffer = "";
	step = 1; *words counter;
	do i=1 to length(model);
		if strip(substr(model, i, 1)) ne " " and i ne length(model) then buffer = strip(buffer)||strip(substr(model, i, 1));
		else if strip(substr(model, i, 1)) = " " or i = length(model) then 
		do;
			buffer = strip(buffer)||strip(substr(model, i, 1));
			model_sp[step] = strip(buffer);
			buffer = "";
			step = step + 1; 
		end;
	end; 
run;

data cars3(drop=step i);
	length model_new $200;
	set cars2;

	array model_sp2 {*} val1-val%eval(&maxcnt);
	model_new=""; *new model. further it will be compared with original var to make sure that nothing was lose; 

	do i=1 to %eval(&maxcnt); *gather values into new var;
		if not missing(model_sp2[i]) then model_new = catx(" ", model_new, model_sp2[i]);
	end;

	if strip(model_new) ne strip(model) then chk_flag = "Y"; *pointed if new var and original var are different;
run;





